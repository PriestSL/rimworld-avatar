<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <Avatar.AvatarEyesDef MayRequire="Sovereign.GobbyWobby">
        <defName>GS_Gobby_Eyes</defName>
        <geneName>GS_Gobby_Eyes</geneName>
        <color1>(1.0,0.8,0.1)</color1>
        <color2>(0.7,0.6,0.0)</color2>
    </Avatar.AvatarEyesDef>

    <Avatar.AvatarHeadboneDef MayRequire="Sov.OrcNormal">
        <defName>GS_Tusk</defName>
        <geneName>GS_Tusk</geneName>
        <unisexPath>Common/Tusk</unisexPath>
        <unisexChildPath>Common/TuskChild</unisexChildPath>
        <unisexNewbornPath>Common/TuskNewborn</unisexNewbornPath>
    </Avatar.AvatarHeadboneDef>

    <Avatar.AvatarFacialDef MayRequire="det.venators">
        <defName>DV_FacePlating</defName>
        <geneName>DV_FacePlating</geneName>
        <replaceModdedTexture>false</replaceModdedTexture>
        <offset>4</offset>
    </Avatar.AvatarFacialDef>

    <Avatar.AvatarEyesDef MayRequire="det.avaloi">
        <defName>DV_Head_Avaloi</defName>
        <geneName>DV_Head_Avaloi</geneName>
        <unisexPath>Common/ThreeEyes</unisexPath>
        <unisexChildPath>Common/ThreeEyesChild</unisexChildPath>
        <eyesPos>
            <li>
                <pos1>(14,27)</pos1>
                <pos2>(15,27)</pos2>
            </li>
            <li>
                <pos1>(24,27)</pos1>
                <pos2>(23,27)</pos2>
            </li>
            <li>
                <pos1>(19,32)</pos1>
                <pos2>(19,31)</pos2>
            </li>
        </eyesPos>
    </Avatar.AvatarEyesDef>

    <Avatar.AvatarHeadboneDef MayRequire="vanillaracesexpanded.pigskin">
        <defName>VRE_MouthTusks</defName>
        <geneName>VRE_MouthTusks</geneName>
        <unisexPath>Common/Tusk</unisexPath>
        <unisexChildPath>Common/TuskChild</unisexChildPath>
        <unisexNewbornPath>Common/TuskNewborn</unisexNewbornPath>
    </Avatar.AvatarHeadboneDef>

    <Avatar.AvatarHeadDef>
        <defName>Head_Skeleton</defName>
        <typeName>SSS_HeadType</typeName>
        <hideTattoo>true</hideTattoo>
        <hideHair>true</hideHair>
        <hideBeard>true</hideBeard>
        <hideWrinkles>true</hideWrinkles>
        <hideEyes>true</hideEyes>
        <hideEars>true</hideEars>
        <hideNose>true</hideNose>
        <hideMouth>true</hideMouth>
        <specialNoJaw>true</specialNoJaw>
        <forceBodyType>Skeleton</forceBodyType>
        <unisexPath>Common/Skull</unisexPath>
    </Avatar.AvatarHeadDef>

    <Avatar.AvatarBodyDef>
        <defName>Body_Skeleton</defName>
        <typeName>Skeleton</typeName>
        <unisexPath>Common/Skeleton</unisexPath>
    </Avatar.AvatarBodyDef>

</Defs>
